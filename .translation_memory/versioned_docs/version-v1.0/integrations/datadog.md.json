      }
    },
    {
      "segment_id": "2df4f75f",
      "source_content": "![Datadog](/img/integrations/datadog/datadog-2.png)",
      "source_content_hash": "1b9d8121c081f4d756c0a40ef0bcdfd4e02f7fd1db6ff68d196816c6e586605c",
      "node_type": "paragraph",
