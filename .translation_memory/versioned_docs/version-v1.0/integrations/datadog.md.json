      }
    },
    {
      "segment_id": "98cfbd53",
      "source_content": "Flag change events will now be sent to Datadog.",
      "source_content_hash": "a37bdb68f6e06a4f5a9e2eb2d7c2e2f9efd77fd629edaf990531873370b3a5d4",
      "node_type": "paragraph",
      "translatable": true,
      "translations": {
        "zh-CN": "功能标志变更事件现在将自动推送至Datadog。"
      }
